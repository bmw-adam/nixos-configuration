{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TpvVyberDb": "Host=yb-tserver-0.yb-tservers.default.svc.cluster.local;Port=5433;Database=tpvvyberdb;Username=tpv;Password=<YSQL_PASSWORD>;Load Balance Hosts=true;Timeout=0;Topology Keys=cloud1.datacenter1.rack1;"
  }
}
